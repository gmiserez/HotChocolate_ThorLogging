{
  "Tracing": {
    "ApplicationId": "1",
    "Level": "Warning",
    "BlobStorage": {
      "ConnectionString": "UseDevelopmentStorage=true"
    },
    "EventHub": {
      "ConnectionString": "<insert your eventHub connectionString here>",
      "TransportType": "AmqpWebSockets"
    },
    "ApplicationRootPath": null,
    "InProcess": "true",
    "Enabled": "true",
    "AllowedPrefixes": [
      "Thor"
    ],
    "Forwarding": [
      {
        "Regex": "Microsoft.AspNetCore.Hosting.Internal.WebHost",
        "Level": "Warning"
      }
    ],
    "SkipRequestFilter": "_health",
    "Events": {
      "Buffer": {
        "Size": "1000"
      }
    },
    "Attachments": {
      "Buffer": {
        "Size": "1000"
      }
    }
  }
}